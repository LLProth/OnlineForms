@model OnlineForms.ViewModels.SFN60026DisplayViewModel


@{
    ViewBag.Title = "Print";
}

@section topHeader {
    @Html.Partial("~/Views/Shared/_FormHeader.cshtml", (Dictionary<string, string>)ViewBag.FormInfo)
}
<table id="printTable">
